<quiz>
    <round name="General Science">
        <question>
            <text>What is the chemical symbol for water?</text>
            <option>H2O</option>
            <option>CO2</option>
            <option>O2</option>
            <option>H2</option>
            <answer>H2O</answer>
        </question>
        <question>
            <text>What planet is known as the Red Planet?</text>
            <option>Earth</option>
            <option>Mars</option>
            <option>Venus</option>
            <option>Jupiter</option>
            <answer>Mars</answer>
        </question>
        <question>
            <text>What is the powerhouse of the cell?</text>
            <option>Nucleus</option>
            <option>Ribosome</option>
            <option>Mitochondria</option>
            <option>Golgi apparatus</option>
            <answer>Mitochondria</answer>
        </question>
        <question>
            <text>Which gas is essential for human respiration?</text>
            <option>Carbon Dioxide</option>
            <option>Oxygen</option>
            <option>Nitrogen</option>
            <option>Hydrogen</option>
            <answer>Oxygen</answer>
        </question>
        <question>
            <text>Which metal is liquid at room temperature?</text>
            <option>Gold</option>
            <option>Mercury</option>
            <option>Iron</option>
            <option>Silver</option>
            <answer>Mercury</answer>
        </question>
        <question>
            <text>What is the speed of light in vacuum?</text>
            <option>300,000 km/s</option>
            <option>150,000 km/s</option>
            <option>1,000 km/s</option>
            <option>500,000 km/s</option>
            <answer>300,000 km/s</answer>
        </question>
        <question>
            <text>Which part of the plant conducts photosynthesis?</text>
            <option>Roots</option>
            <option>Stem</option>
            <option>Leaves</option>
            <option>Flowers</option>
            <answer>Leaves</answer>
        </question>
        <question>
            <text>Which vitamin is produced in the human body when exposed to sunlight?</text>
            <option>Vitamin A</option>
            <option>Vitamin C</option>
            <option>Vitamin D</option>
            <option>Vitamin K</option>
            <answer>Vitamin D</answer>
        </question>
        <question>
        <text>Which force keeps the planets in orbit around the Sun?</text>
        <option>Magnetic force</option>
        <option>Gravity</option>
        <option>Friction</option>
        <option>Nuclear force</option>
        <answer>Gravity</answer>
    </question>

    <question>
        <text>Which scientist developed the theory of general relativity?</text>
        <option>Isaac Newton</option>
        <option>Albert Einstein</option>
        <option>Galileo Galilei</option>
        <option>Nikola Tesla</option>
        <answer>Albert Einstein</answer>
    </question>
    </round>

